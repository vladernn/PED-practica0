#include<iostream>
#include"tvectorporo.h"
using namespace std;

void TVectorPoro::Copia(TVectorPoro &suVectorPoro)
{

}
TVectorPoro::TVectorPoro():error()
{
	this->dimension=0;
	this->datos=NULL;
}

TVectorPoro::TVectorPoro(int x):error()
{
	if(x<=0)
	{
		this->dimension=0;
		this->datos=NULL;
	}else
	{
		this->dimension=x;
		this->datos=NULL;
	}
}
TVectorPoro::TVectorPoro(TVectorPoro cons &Origen):
{
	Copia(suVectorPoro);
}

